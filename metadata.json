{
  "name": "warrenpnz-aide",
  "version": "1.0.3",
  "summary": "A puppet module for installing and configuring AIDE (Advanced Intrustion Detection Environment).",
  "author": "Warren Powell",
  "description": "A puppet module for the installation and configuration of the AIDE (Advanced Intrusion Detection System) application",
  "license": "Apache-2.0",
  "dependencies": [
    {
      "name": "puppetlabs/concat",
      "version_requirement":">= 2.0.0 < 5.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement":">=4.0.0 < 5.0.0"
    }
  ],
  "source": "https://github.com/warrenpnz/puppet-aide",
  "project_page": "https://github.com/warrenpnz/puppet-aide",
  "issues_url": "https://github.com/warrenpnz/puppet-aide/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [ "5", "6", "7" ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [ "5", "6", "7" ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [ "6", "7", "8" ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [ "12", "14" ]
    }
  ]
}
